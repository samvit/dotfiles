# To encrypt a file:
# openssl des3 -salt -in file.log -out file.des3
function encrypt () {
  openssl des3 -salt -in $1 -out $2;
}
# >>> prompts for password
# To Decrypt: 
# openssl des3 -d -salt -in file.des3 -out file.txt -k mypassword
function decrypt () {
  openssl des3 -d -salt -in $1 -out $2 -k $3 
}
